@model IEnumerable<Dfe.EarlyYearsQualification.Web.Models.Content.NavigationLinkModel>

<govuk-footer>
    <govuk-footer-meta>
        <govuk-footer-meta-items>
            @foreach (var link in Model)
            {
                <govuk-footer-meta-item href="@link.Href" target=@(link.OpenInNewTab ? "_blank" : "")>@link.DisplayText</govuk-footer-meta-item>
            }
        </govuk-footer-meta-items>
    </govuk-footer-meta> 
</govuk-footer>
