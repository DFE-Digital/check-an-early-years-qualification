@{
    ViewData["Title"] = "There is a problem with the service";
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-full govuk-grid-column-two-thirds-from-desktop">
        <h1 id="accessibility-statement-heading" class="govuk-heading-xl">Sorry, there is a problem with the service</h1>
        <div id="accessibility-statement-body" class="govuk-body">
            <p class="govuk-body">Try again later.</p>
            <p class="govuk-body">We have not saved your answers. When the service is available, you will have to start again.</p>
            <p class="govuk-body">You can download the <a href="#">early years qualifications list (EYQL) spreadsheet</a>.</p>
        </div>
    </div>
</div>